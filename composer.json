{
    "name": "potherca/flysystem-github",
    "description": "Flysystem adapter for Github",
    "keywords": [
        "potherca",
        "flysystem-github",
        "flysystem-adapter",
        "flysystem",
        "github",
        "adapter"
    ],
    "homepage": "https://github.com/potherca/flysystem-github",
    "license": "MIT",
    "authors": [
        {
            "name": "Potherca",
            "email": "potherca@gmail.com",
            "homepage": "http://pother.ca/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "knplabs/github-api": "^1.4",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Potherca\\Flysystem\\Github\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Potherca\\Flysystem\\Github\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "scripts": {
        "test" : "phpunit"
    }
}
